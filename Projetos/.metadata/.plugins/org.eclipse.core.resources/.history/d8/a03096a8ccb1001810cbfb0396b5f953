package br.moura.gui;

public class Messages{
	
	private Integer id;
	private String mens;
	private String category;
	private String userid;
	
	public Integer getId() {return this.id;}
	public void setId(Integer id) {this.id = id;}
	
	public String getMensage() {return this.mens;}
	public void setMensage(String name) {this.mens = mens;}
	
	public String getCategory() {return this.category;}
	public void setUser(String user) {this.category = category;}
	
	public String getPassword() {return this.userid;}
	public void setPassword(String password) {this.userid = usrid;}

}
